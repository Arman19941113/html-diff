{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "allowUnreachableCode": false,
    "lib": ["esnext", "dom"],
    "jsx": "preserve",
    "rootDir": ".",
    "baseUrl": ".",
    "paths": { }
  },
  "include": ["src/**/*.ts"]
}
